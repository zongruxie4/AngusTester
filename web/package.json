{
  "name": "xcan-angustester-web",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0",
    "yarn": "^1.10.0"
  },
  "scripts": {
    "vite:dev:dev": "vite --mode dev",
    "vite:dev:priv": "vite --mode priv",
    "vite:build:dev": "vite build --mode dev",
    "vite:build:prod": "vite build --mode prod",
    "vite:build:priv": "vite build --mode priv",
    "dev": "vite --mode dev",
    "dev:env": "husky install && cross-env mode_env=$npm_config_env edition_type=$npm_config_edition_type zip_dist=$npm_config_zip_dist node NODE_OPTIONS=--max-old-space-size=8192 scripts/build-dev.js",
    "dev:dev": "husky install && cross-env mode_env=dev edition_type=CLOUD_SERVICE node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build-dev.js",
    "dev:prod": "husky install && cross-env mode_env=prod edition_type=CLOUD_SERVICE node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build-dev.js",
    "dev:priv:community": "husky install && cross-env mode_env=priv edition_type=COMMUNITY zip_dist=false node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build-dev.js",
    "dev:priv:enterprise": "husky install && cross-env mode_env=priv edition_type=ENTERPRISE zip_dist=false node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build-dev.js",
    "dev:priv:datacenter": "husky install && cross-env mode_env=priv edition_type=DATACENTER zip_dist=false node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build-dev.js",
    "build:env": "cross-env mode_env=$npm_config_env edition_type=$npm_config_edition_type node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build.js",
    "build:dev": "cross-env mode_env=dev edition_type=CLOUD_SERVICE node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build.js",
    "build:prod": "cross-env mode_env=prod edition_type=CLOUD_SERVICE node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build.js",
    "build:priv:community": "cross-env mode_env=priv edition_type=COMMUNITY node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build.js",
    "build:priv:enterprise": "cross-env mode_env=priv edition_type=ENTERPRISE node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build.js",
    "build:priv:datacenter": "cross-env mode_env=priv edition_type=DATACENTER node NODE_OPTIONS=--max-old-space-size=8192 ./scripts/build.js",
    "fix:js": "eslint --fix --ext \".ts,.tsx,.js,.jsx,.vue\" --ignore-path .eslintignore src/",
    "fix:style": "stylelint src/**/*.{css,less,vue,html} --ignore-path .stylelintignore --fix",
    "fix": "npm run fix:js && npm run fix:style",
    "test": "jest"
  },
